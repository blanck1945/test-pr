{
  "name": "tap-ruleta",
  "description": "Promotional landing site for Tap",
  "version": "1.0.0",
  "private": true,
  "author": "Tap + Ort Students",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "typescript",
    "mide",
    "tap"
  ],
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "3.5.0",
    "gatsby-plugin-google-tagmanager": "^3.10.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-styled-components": "^4.5.0",
    "react": "17.0.2",
    "react-custom-roulette": "^1.1.8",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "styled-components": "^5.3.0",
    "tsconfig-paths-webpack-plugin": "3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.4",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "babel-plugin-root-import": "6.6.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dependency-cruiser": "^10.0.1",
    "eslint": "7.26.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-root-import": "1.0.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "typescript": "4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
